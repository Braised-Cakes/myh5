{
  "name": "myh5-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "server": "node ./server/bin/www",
    "build": "vue-cli-service build",
    "start": "concurrently \"yarn dev\" \"yarn server\"",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "art-template": "^4.12.2",
    "cookie-parser": "^1.4.3",
    "element-ui": "^2.3.4",
    "express-art-template": "^1.0.1",
    "express-session": "^1.15.6",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.14",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.5.0",
    "superagent": "^3.8.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "babel-plugin-component": "^1.1.0",
    "node-sass": "^4.8.3",
    "qiniu": "^7.1.3",
    "sass-loader": "^6.0.7",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ],
    "plugins": [
      [
        "component",
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": false,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
