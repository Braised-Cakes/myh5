  <style>   
    html,
	body {
		width: 100%;
		height: 100%;
		position: relative;
	}
    .swiper-container {
		width: 100%;
		height: 100%;
	}

	.swiper-container .swiper-wrapper {
		width: 100%;
		height: 100%;
		position: absolute;
	}

	.swiper-slide {
		position: relative;
		width: 100%;
		height: 100%;
        overflow:hidden;
	}
  .items{
      border-width : 0;
    }
  </style>
